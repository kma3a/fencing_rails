  <h1 class="intro"> Welcome <%= @coach.name %>!</h1>

<div class="content sign-template col-md-6">
    <h4> Your Teams</h4>
      <ul>
        <% @coach.owned_teams.each do |team|%>
          <li class="list-group-item"><%=link_to team.name, team_path(team), class:"btn-lg" %>  <%= link_to "Edit", edit_team_path(team), class:"btn-sm"%></li>
        <%end%>
      </ul>
  </div>

  <div class="content sign-template col-md-6">
    <h4> Teams You Coach</h4>
   <ul>
     <% @coach.teams.each do |team| %>
        <li class="list-group-item"><%=link_to  team.name, team_path(team), class:"btn-lg" %></li>
     <%end%>
   </ul>
  </div>
  
