<<div class="coach">
<h3><%= @team.name %> Students Roster</h3>
<table class="table table-bordered">
  <thead>
    <td>Name</td>
    <td>Student Key</td>
  </thead>
  <% @team.students.each do |student| %>
  <tr>
    <td><%= student.name%></td>
    <td><%= student.secret_key%></td>
  </tr>
  <%end%>
</table>
</div>
