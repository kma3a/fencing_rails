<h1>Welcome to Fencing Coach!</h1>
<h3>Look at an Event</h3>
<%= form_for(@event, url: event_public_path, html: {method: 'get'}) do |f| %>
  <%= f.text_field(:secret_key, placeholder: 'Event Key')%>
  <%= submit_tag("View Event")%>
<%end%>
<h3>Look at Student Page</h3>
<%= form_for(@student, url: student_public_path, html: {method: 'get'}) do |f| %>
  <%= f.text_field(:secret_key, placeholder: 'Student Key')%>
  <%= submit_tag("View Student")%>
<%end%>
