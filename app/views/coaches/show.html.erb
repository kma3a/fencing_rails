  <h1 class="intro"> Welcome <%= @coach.name %>!</h1>

<div class="col-md-6">
    <h4> Your Teams</h4>
      <ul>
        <% @coach.owned_teams.each do |team|%>
          <li><%=link_to team.name, team_path(team) %> | <%= link_to "Edit", edit_team_path(team) %></li>
        <%end%>
      </ul>
  </div>

  <div class="col-md-6">
    <h4> Teams You Coach</h4>
   <ul>
     <% @coach.teams.each do |team| %>
        <li><%=link_to  team.name, team_path(team) %></li>
     <%end%>
   </ul>
  </div>
  
